{
  "manifest_version": 3,
  "name": "WiFi Auto Login",
  "version": "1.0",
  "description": "Automatically logs into WiFi portal",
  "permissions": ["storage", "activeTab"],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*:// here you can add link *" 
      ],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
